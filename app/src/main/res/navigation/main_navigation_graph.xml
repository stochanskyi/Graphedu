<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/main_navigation_graph"
    app:startDestination="@id/startFragment">

    <fragment
        android:id="@+id/startFragment"
        android:name="com.nulp.graphedu.presentation.fragments.menuFragment.impl.MenuFragment"
        android:label="Main"
        tools:layout="@layout/fragment_menu">

        <action
            android:id="@+id/action_start_to_tabs"
            app:destination="@id/tabsFragment" />
    </fragment>

    <fragment
        android:id="@+id/tabsFragment"
        android:name="com.nulp.graphedu.presentation.fragments.tabFlowFragment.TabsFlowFragment"
        android:label="Tabs"
        tools:layout="@layout/fragment_tabs_flow" />

</navigation>